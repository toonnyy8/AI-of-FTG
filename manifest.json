{
    "manifest_version": 2,
    "name": "AI of FTG",
    "description": "AI of FTG",
    "version": "2.0",
    "browser_action": {
        "default_title": "AI of FTG",
        "default_icon": "icon.png"
    },
    "background": {
        "scripts": [
            "event.js"
        ],
        "persistent": false
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "permissions": [
        "desktopCapture",
        "tabs",
        "http://*/*",
        "https://*/*"
    ]
}