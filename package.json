{
    "name": "ai-of-ftg",
    "version": "1.0.0",
    "description": "以tensorflow.js實作基於深度學習的FTG AI",
    "main": "index.js",
    "dependencies": {
        "@tensorflow/tfjs": "^2.4.0",
        "babylonjs": "^4.1.0",
        "babylonjs-gui": "^4.1.0",
        "babylonjs-loaders": "^4.1.0",
        "cannon": "^0.6.2",
        "core-js": "^3.6.5",
        "hammerjs": "^2.0.8",
        "parcel": "^1.12.4",
        "regenerator-runtime": "^0.13.7"
    },
    "devDependencies": {
        "typescript": "^3.9.7"
    },
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "dev:dddqn_webworker": "parcel ./src/dddqn_webworker/index.html -p 8080 --open",
        "build:dddqn_webworker": "parcel build --no-content-hash  ./src/dddqn_webworker/index.html --out-dir build/dddqn_webworker --public-url=./",
        "dev:prod_mode": "parcel ./src/prod_mode/index.html -p 8080 --open",
        "build:prod_mode": "parcel build --no-content-hash  ./src/prod_mode/index.html --out-dir build/prod_mode --public-url=./",
        "dev:cnn_dddqn": "parcel ./src/cnn_dddqn/index.html ./src/cnn_dddqn/agent.html -p 8080 --open",
        "build:cnn_dddqn": "parcel build --no-content-hash  ./cnn_dddqn/index.html ./cnn_dddqn/agent.html --out-dir build/cnn_dddqn --public-url=./"
    },
